{
  "name": "material-kit-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "start:dev_container": "docker-compose up --build -V",
    "start:prod_container": "docker-compose -f docker-compose.production.yaml up --build -V",
    "start:prod_container_detatch": "docker-compose -f docker-compose.production.yaml up --build -V -d",
    "stop:dev_container": "docker-compose down",
    "stop:prod_container": "docker-compose -f docker-compose.production.yaml down",
    "exec:terminal": "docker exec -it mv-front-suivi-evaluation /bin/sh"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
