    "@mui/lab": "^5.0.0-alpha.80",
    "@mui/material": "^5.6.4",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.27.2",
    "formik": "^2.2.9",
    "next": "12.1.6",
    "polished": "^4.2.2",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-redux": "^8.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "17.0.31",
    "@types/react": "18.0.9",
